<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhangpan.dao.SysDictDao" >
  <resultMap id="sysDictResultMap" type="SysDictType" >
    <id column="id" property="id" />
    <result column="dict_Value" property="dictValue" />
    <result column="dict_Name" property="dictName" />
  </resultMap>
  
  <sql id="userColumns"> * </sql>
  
  <select id="findAll" resultMap="sysDictResultMap">
  	select <include refid="userColumns" /> from sys_user
  </select>
</mapper>